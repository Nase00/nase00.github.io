{
  "name": "nase",
  "version": "1.0.0",
  "description": "GitHub site",
  "repository": {
    "type": "git",
    "url": "git@github.com:Nase00/nase00.github.io.git"
  },
  "main": "application.js",
  "node_test_paths": "./tests/server/**/*.spec.* ./tests/universal/**/*.spec.*",
  "scripts": {
    "start": "npm run dev",
    "clean": "rimraf dist/bundle*",
    "cp:static": "cp ./src/index.html ./dist/index.html && cp CNAME dist/",
    "dev": "npm run cp:static && webpack-dev-server --config webpack/webpack.hot.config.js --color --progress --inline",
    "build": "npm run clean && npm run cp:static && NODE_ENV=production webpack --config webpack/webpack.prod.config.js",
    "deploy": "git subtree push --prefix dist origin master",
    "lint": "eslint --format=node_modules/eslint-formatter-pretty --ext .js,.jsx *.js client server universal environment tests webpack karma",
    "test": "npm run clean && npm run lint && npm run test:runner",
    "test:runner": "NODE_ENV=test karma start karma/karma.conf",
    "test:cov": "karma start karma/karma.conf.cov.js"
  },
  "author": "Sean Owiecki",
